/* Dialog
----------------------------------*/
.ui-dialog { position: relative;  width: 300px; padding:15px; background-color:#666 !important;   border:0px !important; }
.ui-dialog .ui-dialog-titlebar { padding: .5em .3em .3em 1em; position: relative; height:28px;}
.ui-dialog .ui-dialog-title { float: left; margin: .1em 0 .2em; font-size:15px; color:#333333; font-weight:bold;line-height:24px; word-wrap : normal ;  } 
.ui-dialog .ui-dialog-titlebar-close { position: absolute; right: .3em; top: 50%; width: 19px; margin: -10px 0 0 0; padding: 1px; height: 18px; }
.ui-dialog .ui-dialog-titlebar-close span { display: block; margin: 1px; }
.ui-dialog .ui-dialog-titlebar-close:hover, .ui-dialog .ui-dialog-titlebar-close:focus { padding: 0; }
.ui-dialog .ui-dialog-content { border: 0; padding: .5em 1em; background: none;background-color:#FFFFFF; overflow: auto; zoom: 1; }
.ui-dialog .ui-dialog-buttonpane { border-width: 1px 0 0 0; background-image: url(/images/bt-win.png); margin: 0; padding: .3em 1em .5em .4em;border-top:1px solid #CCCCCC;
-moz-border-radius: 0px 0px 4px 4px !important/*{cornerRadius}*/;
-webkit-border-bottom-left-radius: 4px !important;
-webkit-border-bottom-right-radius: 4px !important;
-webkit-border-top-left-radius: 0px !important;
-webkit-border-top-right-radius: 0px !important;	

}
.ui-dialog .ui-dialog-buttonpane button { /* background-image:url(./images/btn-blue.png);  */ background-color: #868686; color:#FFFFFF; font-weight:bold; font-size:10px; float:right; margin: .5em .4em .5em 0; cursor: pointer; padding: .2em .6em .3em .6em; line-height: 1.4em; width:auto; overflow:visible; font-family:helvetica,arial,sans-serif;border: 0px;}
.ui-dialog .ui-resizable-se { width: 14px; height: 14px; right: 3px; bottom: 3px; }
.ui-draggable .ui-dialog-titlebar { cursor: move; }
.ui-dialog .ui-dialog-buttonpane .ui-state-focus { background: #555555 }